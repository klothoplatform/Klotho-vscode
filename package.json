{
  "name": "klotho",
  "displayName": "Klotho",
  "description": "Syntax highlighting for Klotho annotations",
  "publisher": "Klotho",
  "version": "0.0.3",
  "repository": "https://github.com/KlothoPlatform/Klotho-vscode",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "package": "npx vsce package",
    "prepackage": "npx tsc"
  },
  "contributes": {
    "languages": [
      {
        "id": "klotho",
        "aliases": [
          "Klotho",
          "klo"
        ],
        "extensions": []
      }
    ],
    "grammars": [
      {
        "language": "klotho",
        "path": "./syntaxes/klotho-block-comment.json",
        "scopeName": "source.klotho",
        "embeddedLanguages": {
          "meta.embedded.inline.toml": "toml"
        }
      },
      {
        "language": "klotho",
        "path": "./syntaxes/klotho-line-comment.json",
        "scopeName": "source.line-comment.klotho",
        "embeddedLanguages": {
          "meta.embedded.inline.toml": "toml"
        }
      },
      {
        "path": "./syntaxes/klotho-block-comment.injection.json",
        "scopeName": "klotho.injection.block",
        "injectTo": [
          "source.ts",
          "source.tsx",
          "source.jsx",
          "source.js",
          "source.go"
        ]
      },
      {
        "path": "./syntaxes/klotho-line-comment.injection.json",
        "scopeName": "klotho.injection.line-comment",
        "injectTo": [
          "source.dockerfile",
          "source.python",
          "source.yaml",
          "source.ts",
          "source.tsx",
          "source.js",
          "source.jx",
          "source.go"
        ]
      },
      {
        "path": "./syntaxes/toml-klotho.injection.json",
        "scopeName": "toml.injection.klotho",
        "injectTo": [
          "source.klotho"
        ],
        "embeddedLanguages": {
          "meta.embedded.inline.toml": "toml"
        }
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^2.16.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:python",
    "onLanguage:go",
    "onLanguage:yaml",
    "onLanguage:dockerfile"
  ],
  "main": "./out/extension.js"
}
