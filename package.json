{
  "name": "klotho",
  "displayName": "Klotho",
  "description": "Syntax highlighting for Klotho annotations",
  "version": "0.0.1",
  "repository": "https://github.com/KlothoPlatform/Klotho-vscode",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "package": "npx vsce package",
    "prepackage": "npx tsc"
  },
  "contributes": {
    "languages": [
      {
        "id": "klotho",
        "aliases": [
          "Klotho",
          "klo"
        ],
        "extensions": []
      }
    ],
    "grammars": [
      {
        "language": "klotho",
        "path": "./syntaxes/klotho.json",
        "scopeName": "source.klotho",
        "embeddedLanguages": {
          "meta.embedded.inline.toml": "toml"
        }
      },
      {
        "language": "klotho",
        "path": "./syntaxes/dockerfile-klotho.json",
        "scopeName": "source.klotho.dockerfile",
        "embeddedLanguages": {
          "meta.embedded.inline.toml": "toml"
        }
      },
      {
        "path": "./syntaxes/klotho-clike.injection.json",
        "scopeName": "klotho.injection.clike",
        "injectTo": [
          "source.ts",
          "source.js",
          "source.go"
        ]
      },
      {
        "path": "./syntaxes/klotho-number-sign.injection.json",
        "scopeName": "klotho.injection.number-sign",
        "injectTo": [
          "source.python",
          "source.yaml"
        ]
      },
      {
        "path": "./syntaxes/klotho-dockerfile.injection.json",
        "scopeName": "klotho.injection.dockerfile",
        "injectTo": [
          "source.dockerfile"
        ]
      },
      {
        "path": "./syntaxes/toml-klotho.injection.json",
        "scopeName": "toml.injection.klotho",
        "injectTo": [
          "source.klotho"
        ],
        "embeddedLanguages": {
          "meta.embedded.inline.toml": "toml"
        }
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^2.16.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:python",
    "onLanguage:go",
    "onLanguage:yaml",
    "onLanguage:dockerfile"
  ],
  "main": "./out/extension.js"
}
